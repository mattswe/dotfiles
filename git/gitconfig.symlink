[user]
    name = Matt Sweeney
    email = mattswe@gmail.com

[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto

[core]
    excludesfile = ~/.gitignore

[branch]
    autosetuprebase = always

[diff]
    compactionHeuristic = true

[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true

